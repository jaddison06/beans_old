@LinkWithLib(SDL2)

class SDLEventRaw {
    @Initializer()
    void* SEInit()

    @Invalidates()
    @Show(Destroy)
    void SEDestroy()

    @Getter(x)
    int GetX()
    @Getter(y)
    int GetY()
    @Getter(key)
    KeyCode GetKey()
    @Getter(mouseButton)
    MouseButton GetMouseButton()
    @Getter(type)
    EventType GetType()

    int Poll()
}